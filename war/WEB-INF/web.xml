<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>IndexServlet</servlet-name>
		<servlet-class>id.ac.unsyiah.jte.mobile.IndexServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>IndexServlet</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DataServlet</servlet-name>
		<servlet-class>id.ac.unsyiah.jte.mobile.DataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DataServlet</servlet-name>
		<url-pattern>/nama</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DaftarServlet</servlet-name>
		<servlet-class>id.ac.unsyiah.jte.mobile.DaftarServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DaftarServlet</servlet-name>
		<url-pattern>/daftar</url-pattern>
	</servlet-mapping>
</web-app>
